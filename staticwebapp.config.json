{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/favicon.ico", "/images/*", "*.{css,js,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  }
}
